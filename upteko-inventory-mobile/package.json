{
  "name": "upteko-inventory-mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.22.2",
    "@react-native-firebase/app": "^18.9.0",
    "@react-native-firebase/firestore": "^18.9.0",
    "@react-navigation/native": "^6.1.10",
    "@react-navigation/stack": "^6.3.21",
    "babel": "^6.23.0",
    "expo": "~50.0.4",
    "expo-barcode-scanner": "~12.9.3",
    "expo-camera": "~14.0.5",
    "expo-media-library": "~15.9.1",
    "expo-status-bar": "~1.11.1",
    "firebase": "^10.8.0",
    "react": "18.2.0",
    "react-native": "0.73.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.24.5",
    "@testing-library/react-native": "^12.5.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-expo": "^50.0.4",
    "react-native-dotenv": "^3.4.11"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.js$": "babel-jest",
      "node_modules/firebase/.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": []
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "commonjs",
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-transform-private-property-in-object",
        {
          "loose": true
        }
      ]
    ]
  },
  "private": true
}
